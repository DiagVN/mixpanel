{
    "name": "DiagVN/mixpanel",
    "description": "Mix Panel vendor custom bt Diag",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bradley Nguyen",
            "email": "giang.nguyen@diag.vn",
            "homepage": "https://github.com/GiangBeo"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/promises": "^1.5"
    },
    "require-dev": {
        "ext-curl": "*",
        "bamarni/composer-bin-plugin": "^1.4.1",
        "php-http/client-integration-tests": "^3.0",
        "phpunit/phpunit": "^8.5.5 || ^9.3.5",
        "psr/log": "^1.1 || ^2.0 || ^3.0"
    },
    "suggest": {
        "ext-curl": "Required for CURL handler support",
        "ext-intl": "Required for Internationalized Domain Name (IDN) support",
        "psr/log": "Required for using the Log middleware"
    },
    "autoload": {
        "psr-4": {
            "MixPanel\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MixPanel\\Tests\\": "tests/"
        }
    }
}
